{
  "name": "@tamland/imagemin",
  "version": "0.0.1",
  "description": "Preconfigured imagemin function",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && babel src --config-file --out-dir lib",
    "lint": "eslint src test --ext .jsx,.js,.tsx,.ts --ignore-pattern *.d.ts",
    "prelint": "npm run build",
    "prepublishOnly": "npm run test",
    "pretest": "npm run lint",
    "test": "babel-tape-runner ./test/*.js"
  },
  "browserslist": "node 8",
  "engines": {
    "node": ">= 8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/feight/tamland"
  },
  "author": "News Team (https://www.newsteam.co.za)",
  "contributors": [
    {
      "name": "Peter Bodechtel",
      "url": "https://www.sweetlikepete.com"
    }
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/feight/tamland/issues"
  },
  "keywords": [
    "imagemin",
    "tamland",
    "@tamland"
  ],
  "homepage": "https://github.com/feight/tamland/tree/master/packages/imagemin#readme",
  "files": [
    "lib"
  ],
  "dependencies": {
    "imagemin": "^6.1.0",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-optipng": "^7.0.0",
    "imagemin-svgo": "^7.0.0"
  },
  "devDependencies": {
    "@tamland/babel": "^0.0.3",
    "@tamland/eslint": "^0.0.1",
    "babel-tape-runner": "^3.0.0",
    "tape": "^4.10.2"
  }
}
