{
  "name": "@tamland/stylelint-config",
  "version": "0.0.3",
  "description": "Shared config for stylelint",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && babel src --config-file --out-dir lib",
    "lint": "eslint src test --ext .jsx,.js,.tsx,.ts --ignore-pattern *.d.ts",
    "prelint": "npm run build",
    "prepublishOnly": "npm run test",
    "pretest": "npm run lint",
    "stylelint": "stylelint test/*.scss",
    "test": "stylelint-find-new-rules && stylelint test/*.scss && babel-tape-runner ./test/*.js"
  },
  "browserslist": "node 8",
  "engines": {
    "node": ">= 8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/feight/tamland"
  },
  "author": "News Team (https://www.newsteam.co.za)",
  "contributors": [
    {
      "name": "Peter Bodechtel",
      "url": "https://www.sweetlikepete.com"
    }
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/feight/tamland/issues"
  },
  "keywords": [
    "stylelint",
    "stylelintconfig",
    "stylelint-config",
    "tamland",
    "@tamland"
  ],
  "homepage": "https://github.com/feight/tamland/tree/master/packages/stylelint-config#readme",
  "files": [
    "lib"
  ],
  "dependencies": {},
  "devDependencies": {
    "@tamland/babel": "^0.0.3",
    "@tamland/eslint": "^0.0.9",
    "babel-tape-runner": "^3.0.0",
    "stylelint": "^10.1.0",
    "stylelint-a11y": "^1.2.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-declaration-strict-value": "^1.1.3",
    "stylelint-find-new-rules": "^1.0.2",
    "stylelint-high-performance-animation": "^1.2.1",
    "stylelint-no-unsupported-browser-features": "^3.0.2",
    "stylelint-order": "^3.0.0",
    "stylelint-scss": "^3.8.0",
    "stylelint-selector-tag-no-without-class": "^2.0.0",
    "stylelint-use-nesting": "^1.2.1",
    "tape": "^4.10.2"
  },
  "peerDependencies": {
    "stylelint": "^10.1.0",
    "stylelint-a11y": "^1.2.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-declaration-strict-value": "^1.1.3",
    "stylelint-high-performance-animation": "^1.2.1",
    "stylelint-no-unsupported-browser-features": "^3.0.2",
    "stylelint-order": "^3.0.0",
    "stylelint-scss": "^3.8.0",
    "stylelint-selector-tag-no-without-class": "^2.0.0",
    "stylelint-use-nesting": "^1.2.1"
  }
}
