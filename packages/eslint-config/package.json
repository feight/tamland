{
  "name": "@tamland/eslint-config",
  "version": "0.0.7",
  "description": "Shared config for eslint",
  "main": "lib/configs",
  "scripts": {
    "build": "rm -rf lib && babel src --config-file --out-dir lib",
    "lint": "eslint src test --ext .jsx,.js,.tsx,.ts --ignore-pattern *.d.ts",
    "prelint": "npm run build",
    "prepublishOnly": "npm run test",
    "pretest": "npm run lint",
    "test": "babel-tape-runner ./test/*.js",
    "unused": "eslint-find-rules lib/configs/index.js --unused --no-error"
  },
  "browserslist": "node 8",
  "engines": {
    "node": ">= 8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/feight/tamland"
  },
  "author": "News Team (https://www.newsteam.co.za)",
  "contributors": [
    {
      "name": "Peter Bodechtel",
      "url": "https://www.sweetlikepete.com"
    }
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/feight/tamland/issues"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "tamland",
    "@tamland"
  ],
  "homepage": "https://github.com/feight/tamland/tree/master/packages/eslint-config#readme",
  "files": [
    "lib"
  ],
